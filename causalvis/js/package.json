{
  "name": "causalvis",
  "version": "0.1.0",
  "description": "Visualization for causal inference",
  "author": "",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com//causalvis.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
    "jupyterlab-extension"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js"
  ],
  "scripts": {
    "clean": "rimraf dist/ && rimraf ../causalvis/labextension/ && rimraf ../causalvis/nbextension",
    "prepublish": "webpack",
    "build": "webpack",
    "build:prod": "webpack --mode=production && yarn run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "watch": "webpack --watch --mode=development",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.0",
    "rimraf": "^2.6.1",
    "webpack": "^5"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.1 || ^2 || ^3 || ^4",
    "lodash": "^4.17.4",
    "yarn": "^1.22.19"
  },
  "jupyterlab": {
    "extension": "lib/labplugin",
    "outputDir": "../causalvis/labextension",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      }
    }
  }
}
